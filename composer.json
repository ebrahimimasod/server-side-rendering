{
    "name": "medialearn/php-ssr-render",
    "description": "Server side rendering JavaScript in your PHP application",
    "keywords": [
        "server-side-rendering"
    ],
    "homepage": "https://github.com/ebrahimimasod/server-side-rendering",
    "license": "MIT",
    "authors": [
        {
            "name": "Masoud ebrahimi",
            "email": "ebrahimimasod@gmail.com",
           
        }
    ],
    "require": {
       "php": ">=8.2",
        "symfony/process": "^3.0|^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Ssr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Ssr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
